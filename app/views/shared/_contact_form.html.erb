<%= form_for(@contact) do |f| %>
    <%= f.text_field :title, placeholder: "Name..." %>
    <%= f.text_field :email, placeholder: "Email..." %>
    <%= f.text_area :content, placeholder: 'Please fill out the form completely so that we can respond to your request as soon as possible.'%>
  	<%= f.submit "Post", class: "btn btn-large btn-primary signoutbutton" %>
<% end %>
