<%= stylesheet_link_tag "format", media: "all",
                                           "data-turbolinks-track" => true %>
<%= javascript_include_tag "application", "data-turbolinks-track" => true %>


<h1>All posts</h1>

<ul class="posts">
  <% @posts.each do |post| %>
    <li>
	   <%= link_to post.content, post %>
    </li>
  <% end %>
</ul>